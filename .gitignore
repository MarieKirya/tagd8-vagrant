# Ignore paths that contain user-generated content.
docroot/sites/*/files
docroot/sites/*/private
docroot/files/*

# Pantheon commits a settings.php for environment-specific settings.
# Place local settings in settings.local.php
docroot/sites/*/settings.local.php
docroot/sites/*/services*.yml

# Remove local node files for gulp support.
.sass-cache*
node_modules*

# ** Only works in OSs that support newer versions of fnmatch (Bash 4+)
/docroot/sites/default/**/files
/docroot/sites/default/**/private

# Ignore SimpleTest multi-site environment.
sites/simpletest

